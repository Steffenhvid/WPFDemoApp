<UserControl x:Class="WPFTestApplication.Views.ComboBoxView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFTestApplication.Views"
             xmlns:vm="clr-namespace:WPFTestApplication.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="800">
    <Grid>
        <ComboBox
            MaxHeight="25"
            ItemsSource="{Binding ComboBoxData}"
            SelectedItem="{Binding CurrentItem}"
            DisplayMemberPath="PresentationName"
            SelectedValuePath="AccesName"
            SelectedValue="{Binding SelectedValue}"/>
    </Grid>
</UserControl>
